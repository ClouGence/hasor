<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://www.hasor.net/sechma/hasor-dataql">
    <hasor>
        <!-- 包扫描覆盖范围，逗号间隔 -->
        <loadPackages>net.hasor.dataql.*</loadPackages>
        <!-- ApiBinder 扩展（多个 type 可以指定同一个实现类，Hasor 会自动映射到同一个实例对象上） -->
        <apiBinderSet>
            <binder type="net.hasor.dataql.binder.QueryApiBinder">net.hasor.dataql.binder.QueryApiBinderCreator</binder>
        </apiBinderSet>
    </hasor>
</config>