<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.hasor.test.db.dal.Mapper1Dal">

    <resultMap id="tb_user_resultMap" type="net.hasor.test.db.dto.TbUser2">
        <id column="user_uuid" property="uid"/>
        <result column="name" property="name"/>
        <result column="login_name" property="account"/>
        <result column="login_password" property="password"/>
        <result column="email" property="mail"/>
        <result column="index" property="index" javaType="int"/>
        <result column="register_time" property="createTime"/>
    </resultMap>

    <dataqlMap id="tb_user_dataqlMap" type="net.hasor.test.db.dto.TbUser2">
        return #;
    </dataqlMap>


    <sql id="projectInfoDO_allColumns">
        id
        , owner_id
    </sql>

    <!-- dataqlMap="tb_user_dataqlMap"-->
    <select id="testBind" resultMap="tb_user_resultMap">
        SELECT
        <include refid="projectInfoDO_allColumns"/>
        FROM console_job
        WHERE aac = #{abc}
    </select>

</mapper>
