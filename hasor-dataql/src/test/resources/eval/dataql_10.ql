
// 说明：基础四则运算，大数四则运算演示。

option MAX_DECIMAL_DIGITS   = 5  // 小数精度为 5

// 基础算数运算
var basicNumber = [
    5 + 3,
    5 - 3,
    5 * 3,
    5 / 3,
    5 \ 3,
    5 % 3
];

// double算数运算
var doubleNumber = [
    double()~ + 3,
    double()~ - 3,
    double()~ * 3,
    double()~ / 3,
    double()~ \ 3,
    double()~ % 3
];

// 大数四则运算
var bigNumber = [
    999999999999999999999999999999999999999999999999999999999999 + 8888888888888888888888888888888888888,
    999999999999999999999999999999999999999999999999999999999999 - 8888888888888888888888888888888888888,
    999999999999999999999999999999999999999999999999999999999999 * 8888888888888888888888888888888888888,
    999999999999999999999999999999999999999999999999999999999999 / 8888888888888888888888888888888888888,
    999999999999999999999999999999999999999999999999999999999999 \ 8888888888888888888888888888888888888,
    999999999999999999999999999999999999999999999999999999999999 % 8888888888888888888888888888888888888
];

return {
    "basic"  : basicNumber,
    "double" : doubleNumber,
    "big"    : bigNumber
};